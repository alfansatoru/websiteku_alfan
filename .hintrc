{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off"
      }
    ],
    "button-type": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off",
        "image-alt": "off",
        "document-title": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "detect-css-reflows/composite": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 16.4",
    "not safari <= 16.4"
  ]
}